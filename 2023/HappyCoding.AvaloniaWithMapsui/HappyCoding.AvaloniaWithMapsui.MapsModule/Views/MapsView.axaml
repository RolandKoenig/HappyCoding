<ext:MvvmUserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ext="https://github.com/RolandK.AvaloniaExtensions"
             xmlns:mapsui="clr-namespace:Mapsui.UI.Avalonia;assembly=Mapsui.UI.Avalonia"
             xmlns:local="clr-namespace:HappyCoding.AvaloniaWithMapsui.MapsModule.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="HappyCoding.AvaloniaWithMapsui.MapsModule.Views.MapsView"
             DataContext="{ext:CreateUsingDependencyInjection {x:Type local:MapsViewModel}}">
    <mapsui:MapControl x:Name="MapControl"
                       PointerPressed="OnMapControl_PointerPressed"
                       PointerReleased="OnMapControl_PointerReleased"
                       PointerMoved="OnMapControl_PointerMoved"/>
</ext:MvvmUserControl>
